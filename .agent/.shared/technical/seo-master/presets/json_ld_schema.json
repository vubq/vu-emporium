{
  "name": "Standard JSON-LD Schemas",
  "description": "Google-compliant structured data templates.",
  "templates": {
    "article": {
      "@context": "https://schema.org",
      "@type": "Article",
      "headline": "{{TITLE}}",
      "image": "{{IMAGE_URL}}",
      "author": {
        "@type": "Person",
        "name": "{{AUTHOR_NAME}}"
      },
      "datePublished": "{{DATE}}"
    },
    "breadcrumbs": {
      "@context": "https://schema.org",
      "@type": "BreadcrumbList",
      "itemListElement": []
    },
    "software": {
      "@context": "https://schema.org",
      "@type": "SoftwareApplication",
      "name": "{{APP_NAME}}",
      "applicationCategory": "DeveloperApplication",
      "operatingSystem": "Web"
    }
  }
}
